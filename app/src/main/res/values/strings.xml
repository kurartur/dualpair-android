    <string name="create_db"><![CDATA[

        DROP TABLE IF EXISTS users;
        CREATE TABLE IF NOT EXISTS users (
            _id BIGINT,
            description TEXT NULL,
            date_of_birth DATE NULL,
            age INT NULL,
            name VARCHAR(128) NULL,
            status SMALLINT NOT NULL DEFAULT 0,
            gender VARCHAR(1) NULL,
            update_time TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
            PRIMARY KEY (_id));

        DROP TABLE IF EXISTS search_parameters;
        CREATE TABLE IF NOT EXISTS search_parameters (
            _id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
            min_age INT NULL,
            max_age INT NULL,
            search_female VARCHAR(1) NULL,
            search_male VARCHAR(1) NULL,
            sync_status VARCHAR(1) NOT NULL DEFAULT \'C\');

        DROP TABLE IF EXISTS user_sociotypes;
        CREATE TABLE IF NOT EXISTS user_sociotypes (
          _id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
          user_id BIGINT NOT NULL,
          code1 VARCHAR(45) NOT NULL,
          code2 VARCHAR(45) NOT NULL);

        DROP TABLE IF EXISTS matches;
        CREATE TABLE IF NOT EXISTS matches (
          _id BIGINT NOT NULL,
          distance INT NOT NULL,
          create_time TIMESTAMP NOT NULL,
          PRIMARY KEY (_id));

        DROP TABLE IF EXISTS user_accounts;
        CREATE TABLE IF NOT EXISTS user_accounts (
          _id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
          user_id BIGINT NOT NULL,
          account_id TEXT NOT NULL,
          account_type VARCHAR(2) NOT NULL);

        DROP TABLE IF EXISTS user_locations;
        CREATE TABLE IF NOT EXISTS user_locations (
          _id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL,
          user_id BIGINT NOT NULL,
          latitude DOUBLE NULL,
          longitude DOUBLE NULL,
          country_code VARCHAR(2) NOT NULL,
          city VARCHAR(128) NOT NULL);

        DROP TABLE IF EXISTS user_photos;
        CREATE TABLE IF NOT EXISTS user_photos (
          _id BIGINT PRIMARY KEY NOT NULL,
          user_id BIGINT NOT NULL,
          account_type VARCHAR(2) NOT NULL,
          id_on_account VARCHAR(45) NULL,
          source_link TEXT NULL,
          position SMALLINT NOT NULL DEFAULT 0);

        DROP TABLE IF EXISTS match_parties;
        CREATE TABLE IF NOT EXISTS match_parties (
          _id BIGINT NOT NULL,
          match_id BIGINT NOT NULL,
          user_id BIGINT NOT NULL,
          response VARCHAR NOT NULL,
          sync_status VARCHAR(1) NOT NULL DEFAULT \'C\',
          PRIMARY KEY (_id));

    ]]></string>
